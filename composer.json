{
    "name": "hof-university/blitz-auto-site",
    "description": "Blitz Auto Typo3",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        }
    ],
    "require": {
        "typo3/minimal": "^9.5",
        "hof-university/blitz-auto": "^1.0",
        "typo3/cms-adminpanel": "^9.5",
        "typo3/cms-backend": "^9.5",
        "typo3/cms-core": "^9.5",
        "typo3/cms-extbase": "^9.5",
        "typo3/cms-extensionmanager": "^9.5",
        "typo3/cms-filelist": "^9.5",
        "typo3/cms-fluid": "^9.5",
        "typo3/cms-fluid-styled-content": "^9.5",
        "typo3/cms-form": "^9.5",
        "typo3/cms-frontend": "^9.5",
        "typo3/cms-install": "^9.5",
        "typo3/cms-recordlist": "^9.5",
        "typo3/cms-recycler": "^9.5",
        "typo3/cms-redirects": "^9.5",
        "typo3/cms-reports": "^9.5",
        "typo3/cms-setup": "^9.5",
        "typo3/cms-t3editor": "^9.5",
        "typo3/cms-tstemplate": "^9.5",
        "typo3/cms-viewpage": "^9.5",
        "cweagans/composer-patches": "^1.6",
        "typo3/cms-felogin": "^9.5",
        "vlucas/phpdotenv": "^2.5",
        "symfony/dotenv": "^4.2",
        "helhum/typo3-console": "^5.6",
        "hofuniversity/sitemanagement": "^1.0",
        "hofuniversity/notifications": "^1.0"
    },
    "extra": {
        "patches": {
            "bk2k/bootstrap-package": {
                "Processes symlinked SCSS sources": "https://patch-diff.githubusercontent.com/raw/benjaminkott/bootstrap_package/pull/585.patch"
            }
        }
    },
    "require-dev": {
        "friendsoftypo3/extension-builder": "dev-master"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "7deepakb",
            "email": "deepak.balaram7@gmail.com"
        }
    ]
}
